USE CINE_NUEVACORDOBA\par
GO\par
\par
CREATE TABLE SEXOS(\par
ID_SEXO INT,\par
SEXO VARCHAR (30)\par
CONSTRAINT PK_ID_SEXO PRIMARY KEY (ID_SEXO)\par
)\par
\par
CREATE TABLE PROVINCIAS(\par
ID_PROVINCIA INT,\par
NOM_PROVINCIA VARCHAR (50),\par
CONSTRAINT PK_ID_PROVINCIA PRIMARY KEY (ID_PROVINCIA)\par
)\par
\par
CREATE TABLE LOCALIDADES(\par
ID_LOCALIDAD INT,\par
NOM_LOCALIDAD VARCHAR(50),\par
ID_PROVINCIA INT\par
CONSTRAINT PK_LOCALIDAD PRIMARY KEY (ID_LOCALIDAD),\par
CONSTRAINT FK_PROVINCIA FOREIGN KEY (ID_PROVINCIA)\par
REFERENCES PROVINCIAS (ID_PROVINCIA)\par
)\par
\par
CREATE TABLE BARRIOS(\par
ID_BARRIO INT,\par
NOM_BARRIO VARCHAR (50),\par
ID_LOCALIDAD INT\par
CONSTRAINT PK_BARRIO PRIMARY KEY (ID_BARRIO),\par
CONSTRAINT FK_LOCALIDAD FOREIGN KEY (ID_LOCALIDAD)\par
REFERENCES LOCALIDADES (ID_LOCALIDAD)\par
)\par
\par
CREATE TABLE TIPOS_DOCUMENTOS(\par
ID_TIPO_DOC INT,\par
TIPO_DOC VARCHAR(50)\par
CONSTRAINT PK_TIPO_DOC PRIMARY KEY (ID_TIPO_DOC)\par
)\par
\par
CREATE TABLE CLIENTES(\par
ID_CLIENTE INT,\par
NOM_CLIENTE VARCHAR(50),\par
APE_CLIENTE VARCHAR(50),\par
ID_TIPO_DOC INT,\par
NUM_DOC INT,\par
FECHA_NAC DATE,\par
ID_SEXO INT,\par
ID_BARRIO INT,\par
DOMICILIO VARCHAR(50)\par
CONSTRAINT PK_CLIENTE PRIMARY KEY (ID_CLIENTE),\par
CONSTRAINT FK_TIPO_DOC FOREIGN KEY (ID_TIPO_DOC)\par
REFERENCES TIPOS_DOCUMENTOS (ID_TIPO_DOC),\par
CONSTRAINT FK_SEXO FOREIGN KEY (ID_SEXO)\par
REFERENCES SEXOS (ID_SEXO),\par
CONSTRAINT FK_BARRIO_CLIENTE FOREIGN KEY (ID_BARRIO)\par
REFERENCES BARRIOS (ID_BARRIO)\par
)\par
\par
CREATE TABLE TIPOS_CONTACTOS(\par
ID_TIPO_CONTACTO INT,\par
TIPO_CONTACTO VARCHAR (100)\par
CONSTRAINT PK_TIPO_CONTACTO PRIMARY KEY (ID_TIPO_CONTACTO)\par
)\par
\par
CREATE TABLE CONTACTOS(\par
ID_CONTACTO INT,\par
ID_CLIENTE INT,\par
ID_TIPO_CONTACTO INT,\par
DETALLE_CONTACTO VARCHAR(50)\par
CONSTRAINT PK_CONTACTO PRIMARY KEY (ID_CONTACTO),\par
CONSTRAINT FK_CLIENTE_CONTACTO FOREIGN KEY (ID_CLIENTE)\par
REFERENCES CLIENTES (ID_CLIENTE),\par
CONSTRAINT FK_TIPO_CONTACTO FOREIGN KEY (ID_TIPO_CONTACTO)\par
REFERENCES TIPOS_CONTACTOS (ID_TIPO_CONTACTO)\par
)\par
\par
CREATE TABLE GENEROS(\par
ID_GENERO INT,\par
GENERO VARCHAR(50),\par
CONSTRAINT PK_GENERO PRIMARY KEY (ID_GENERO)\par
)\par
\par
CREATE TABLE RESTRICCIONES(\par
ID_RESTRICCION INT,\par
RESTRICCION VARCHAR(50)\par
CONSTRAINT PK_RESTRICCION PRIMARY KEY (ID_RESTRICCION)\par
)\par
\par
CREATE TABLE TIPOS_SALAS(\par
ID_TIPO_SALA INT,\par
TIPO_SALA VARCHAR(50)\par
CONSTRAINT PK_TIPO_SALA PRIMARY KEY (ID_TIPO_SALA)\par
)\par
\par
CREATE TABLE SALAS(\par
ID_SALA INT,\par
NOMBRE_SALA VARCHAR(50),\par
CAPACIDAD INT,\par
ID_TIPO_SALA INT\par
CONSTRAINT PK_SALA PRIMARY KEY (ID_SALA),\par
CONSTRAINT FK_TIPO_SALA FOREIGN KEY (ID_TIPO_SALA)\par
REFERENCES TIPOS_SALAS (ID_TIPO_SALA)\par
)\par
\par
CREATE TABLE PELICULAS(\par
ID_PELICULA INT,\par
NOMBRE VARCHAR (100),\par
DURACION INT,\par
ID_RESTRICCION INT,\par
ID_GENERO INT\par
CONSTRAINT PK_PELICULA PRIMARY KEY (ID_PELICULA),\par
CONSTRAINT FK_RESTRICCION FOREIGN KEY (ID_RESTRICCION)\par
REFERENCES RESTRICCIONES (ID_RESTRICCION),\par
CONSTRAINT FK_GENERO FOREIGN KEY (ID_GENERO)\par
REFERENCES GENEROS (ID_GENERO)\par
)\par
\par
CREATE TABLE BUTACAS(\par
ID_BUTACA INT,\par
NUMERO INT,\par
FILA VARCHAR(3),\par
ID_SALA INT\par
CONSTRAINT PK_BUTACA PRIMARY KEY (ID_BUTACA),\par
CONSTRAINT FK_SALA FOREIGN KEY (ID_SALA)\par
REFERENCES SALAS (ID_SALA)\par
)\par
\par
CREATE TABLE FUNCIONES (\par
ID_FUNCION INT,\par
ID_PELICULA INT,\par
ID_SALA INT,\par
FECHA_FUNCION DATE,\par
HORA_FUNCION TIME\par
CONSTRAINT PK_FUNCION PRIMARY KEY (ID_FUNCION),\par
CONSTRAINT FK_PELICULA_FUNCION FOREIGN KEY (ID_PELICULA)\par
REFERENCES PELICULAS (ID_PELICULA),\par
CONSTRAINT FK_SALA_FUNCIONES FOREIGN KEY (ID_SALA)\par
REFERENCES SALAS (ID_SALA)\par
)\par
\par
CREATE TABLE ESTADOS_RESERVAS(\par
ID_ESTADO_RESERVA INT,\par
ESTADO VARCHAR(50)\par
CONSTRAINT PK_ESTADO_RESERVA PRIMARY KEY (ID_ESTADO_RESERVA)\par
);\par
\par
\par
CREATE TABLE RESERVAS(\par
ID_RESERVA INT,\par
FECHA DATE,\par
ID_CLIENTE INT,\par
ID_ESTADO_RESERVA INT\par
CONSTRAINT PK_RESERVA PRIMARY KEY (ID_RESERVA),\par
CONSTRAINT FK_CLIENTE FOREIGN KEY (ID_CLIENTE)\par
REFERENCES CLIENTES (ID_CLIENTE),\par
CONSTRAINT FK_ESTADO_RESERV FOREIGN KEY (ID_ESTADO_RESERVA)\par
REFERENCES ESTADOS_RESERVAS (ID_ESTADO_RESERVA)\par
)\par
\par
CREATE TABLE TICKETS(\par
NUM_TICKET INT,\par
ID_FUNCION INT,\par
ID_BUTACA INT,\par
ID_RESERVA INT NULL,\par
PRECIO DECIMAL (10,2),\par
FECHA DATE\par
CONSTRAINT PK_TICKET PRIMARY KEY (NUM_TICKET),\par
CONSTRAINT FK_FUNCION FOREIGN KEY (ID_FUNCION)\par
REFERENCES FUNCIONES (ID_FUNCION),\par
CONSTRAINT FK_BUTACA FOREIGN KEY (ID_BUTACA)\par
REFERENCES BUTACAS (ID_BUTACA),\par
CONSTRAINT FK_RESERVAS FOREIGN KEY (ID_RESERVA)\par
REFERENCES RESERVAS (ID_RESERVA));\par
\par
\par
CREATE TABLE TIPOS_COMPRAS(\par
ID_TIPO_COMPRA INT,\par
TIPO_COMPRA VARCHAR (30)\par
CONSTRAINT PK_TIPO_COMPRA PRIMARY KEY (ID_TIPO_COMPRA)\par
)\par
\par
CREATE TABLE FACTURAS(\par
NUM_FACTURA INT,\par
ID_CLIENTE INT,\par
FECHA_FACTURA DATE,\par
ID_TIPO_COMPRA INT\par
CONSTRAINT PK_FACTURA PRIMARY KEY (NUM_FACTURA),\par
CONSTRAINT FK_CLIENTES FOREIGN KEY (ID_CLIENTE)\par
REFERENCES CLIENTES (ID_CLIENTE),\par
CONSTRAINT FK_TIPO_COMPRA FOREIGN KEY (ID_TIPO_COMPRA)\par
REFERENCES TIPOS_COMPRAS (ID_TIPO_COMPRA)\par
)\par
\par
CREATE TABLE DETALLE_FACTURAS(\par
ID_DETALLE_FACTURA INT,\par
NUM_FACTURA INT,\par
NUM_TICKET INT,\par
CANTIDAD INT,\par
PRECIO DECIMAL (10,2)\par
CONSTRAINT PK_DETALLE PRIMARY KEY (ID_DETALLE_FACTURA),\par
CONSTRAINT FK_FACTURAS FOREIGN KEY (NUM_FACTURA)\par
REFERENCES FACTURAS (NUM_FACTURA),\par
CONSTRAINT FK_TICKET FOREIGN KEY (NUM_TICKET)\par
REFERENCES TICKETS (NUM_TICKET))\par
\par
\par
